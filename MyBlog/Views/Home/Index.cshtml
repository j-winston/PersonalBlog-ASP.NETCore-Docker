@model IEnumerable<MyBlog.Models.Post>
@{
	Layout = "_Layout";
}

<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>James Winston - Software Developer in Morrisville, North Carolina</title>
	<link rel="stylesheet" href="/lib/css/styles.css">
	
	<div class="post-container-main">

		@foreach(var post in Model)
		{
			<div class="blog-post">
			
				<p class="post-title"><a href="@Url.Action("Details", "Home", new{slug=post.Slug})">@post.Title</a></p>
				<p class="post-date">@post.AuthoredDate</p>
				<p class="post-content">@post.Content</p>
			</div>
		}
	</div>



</head>
<body>


	
</body>
</html>
